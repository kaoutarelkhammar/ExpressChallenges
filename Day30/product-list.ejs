<!DOCTYPE html>
<html>
<head>
  <title>Product List</title>
  <link rel="stylesheet" type="text/css" href="/styles.css">

</head>
<body>
  <h1>Product List</h1>
  <ul>
    <% products.forEach(function(product) { %>
      <li><a href="/products/<%= product.id %>"><%= product.name %></a></li>
    <% }); %>
  </ul>
</body>
</html>
